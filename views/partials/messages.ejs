<% if(typeof errors != 'undefined') { %>
<% errors.forEach((err) => { %>
<div class="alert alert-danger" role="alert">
<%= err.msg %>
</div>
<% }) %>
<% } %>

<% if(success != '') { %>
<div class="alert alert-success" role="alert">
<%= success  %>
</div>
<% } %>

<% if(error != '') { %>
<div class="alert alert-danger" role="alert">
<%= error  %>
</div>
<% } %>

<% if(err != '') { %>
<div class="alert alert-danger" role="alert">                          <%= err %>
</div>
<% } %>

